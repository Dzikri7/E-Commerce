{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/v1/auth",
        "dest": "/"
      },
      {
        "src": "/api/v1/category",
        "dest": "/"
      },
      {
        "src": "/api/v1/product",
        "dest": "/"
      }
    ]
  }
  